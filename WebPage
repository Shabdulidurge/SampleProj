package com.test.demo

String filepath = "C:\\Windows\\System32\\config\\systemprofile\\git\\repository\\Groovy1\\src\\com\\test\\demo\\WebPage.groovy"
File myFile = new File(filepath)
myFile.withReader { reader ->
	while((line = reader.readLine()) != null) {
		println "$line"
		println line.length()
	}
	
	
}
